{
  "name": "Improved Bitbucket Pull Requests",
  "version": "1.0",
  "description": "Improved Bitbucket Pull Requests",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://bitbucket.org/*/pull-requests/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "focus.png"
  }
}
